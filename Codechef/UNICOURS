#include<iostream>
using namespace std;
int main()
{
    int T;
    cin>>T;
    for(int i=0; i<T; i++)
    {
        int max=0,N;
        cin>>N;
        for(int j=0; j<N; j++)
        {
            int x;
            cin>>x;
            if(x>max)
            {
                max=x;
            }
        }
        cout<<N-max<<endl;
    }
}
